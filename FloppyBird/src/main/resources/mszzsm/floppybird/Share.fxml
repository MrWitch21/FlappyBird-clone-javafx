<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="650.0" prefWidth="450.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mszzsm.floppybird.controllers.share.ShareController">
    <ImageView fitHeight="141.0" fitWidth="456.0" layoutY="560.0">
        <Image url="@../../floor.png" />
        <smooth>true</smooth>
    </ImageView>
    <ImageView fitHeight="757.0" fitWidth="456.0" layoutY="-190.0" nodeOrientation="INHERIT">
        <image>
            <Image url="@../../background.png" />
        </image>
    </ImageView>
   <ImageView fitHeight="77.0" fitWidth="282.0" layoutX="87.0" layoutY="66.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../title.png" />
      </image>
   </ImageView>
    <TextField fx:id="input" layoutX="81.0" layoutY="262.0" prefHeight="36.0" prefWidth="295.0">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
    </TextField>
   <Button layoutX="178.0" layoutY="309.0" mnemonicParsing="false" onAction="#saveToDB" prefHeight="30.0" prefWidth="100.0" styleClass="arcade-button" stylesheets="@style.css">
      <graphic>
         <ImageView fitHeight="42.0" fitWidth="123.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../Share.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button layoutX="160.0" layoutY="502.0" mnemonicParsing="false" onAction="#switchToMain" prefHeight="31.0" prefWidth="98.0" styleClass="arcade-button" stylesheets="@style.css">
      <graphic>
         <ImageView fitHeight="70.0" fitWidth="137.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../BackToMainMenu.png" />
            </image>
         </ImageView>
      </graphic></Button>
   <Label fx:id="message" layoutX="81.0" layoutY="214.0" prefHeight="30.0" prefWidth="295.0" stylesheets="@style.css" textFill="RED">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <Label layoutX="81.0" layoutY="244.0" prefHeight="17.0" prefWidth="282.0" text="Enter your username (max 128 char.)" />
</AnchorPane>
